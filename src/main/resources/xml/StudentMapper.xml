<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wkcto.plus.mapper.StudentMapper">
<select id="selectStudentById" parameterType="java.lang.Integer" resultType="com.wkcto.plus.entity.Student">

 select id,name,age,email,status from student where id=#{id}

</select>
